# Server Configuration
PORT=5000
NODE_ENV=development

# Database
MONGODB_URI=mongodb://localhost:27017/insurance-claims
CORS_ORIGINS="*"
# AWS S3 Configuration
AWS_ACCESS_KEY_ID=your-access-key
